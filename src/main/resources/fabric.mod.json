    "homepage": "https://modrinth.com/mod/apollo",
    "sources": "https://github.com/RyanThePinkHacker/Apollo-MC.git",
    "issues": "https://github.com/RyanThePinkHacker/Apollo-MC/issues"
  },
  "license": [
    "LGPL-3.0-only",
    "Assets: All Rights Reserved"
  ],
  "icon": "assets/apollo/textures/misc/icon.png",
