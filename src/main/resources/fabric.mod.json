{
  "schemaVersion": 1,
  "id": "apollo",
  "version": "${version}",
  "name": "Apollo",
  "description": "A space mod for Minecraft.",
  "authors": [
    {
      "name": "Ryan",
      "contact": {
        "email": "ryan@thepinkhacker.com",
        "homepage": "https://thepinkhacker.com"
      }
    }
  ],
